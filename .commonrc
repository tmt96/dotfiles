alias em='emacsclient -c '
alias et='emacsclient -t'
export ALTERNATE_EDITOR=""
export EDITOR="et"         # $EDITOR opens in terminal
export VISUAL="em"         # $VISUAL opens in GUI mode
export CLICOLOR=1
export LSCOLORS=gxBxhxDxfxhxhxhxhxcxcx
alias ls='ls -GFh'
export TERM=xterm-256color

export PATH=/usr/local/bin:/usr/local/sbin:~/bin:/usr/local/bin:/usr/local/sbin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/share/dotnet:/Library/Frameworks/Mono.framework/Versions/Current/Commands:/Library/TeX/texbin:"$PATH"
export PATH="$PATH":/usr/local/anaconda3/bin:~/.dotnet/tool
export PATH="/usr/local/opt/curl/bin:$PATH"
source $HOME/.cargo/env
